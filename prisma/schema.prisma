// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Float
  address     String
  city        String
  createdAt   DateTime @default(now())
}
