generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Property {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Float
  address     String
  city        String
  createdAt   DateTime @default(now())
}
