datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  role     String @default("tenant")
}

model Property {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Int
  address     String
  city        String
  createdAt   DateTime @default(now())
}
